@model ArtShare.Models.SearchModel
@{
    ViewBag.Title = "Music";
}

<h2>Music</h2>

<section class="Collage effect-parent">

    @if (Model.Music != null)
    {
        foreach (var m in Model.Music)
        {

            <div class="Image_Wrapper" data-caption="@m.Title" onclick="location.href='@Url.Action("Index", "Details", new{Id = m.ProductId})'" style="cursor:pointer">
                @if (m.Thumbnail != null)
                {
                    <img src="@m.Thumbnail">
                }
                else
                {
                    <img src="http://placehold.it/350x150" />
                }
            </div>
        }
    }

</section>