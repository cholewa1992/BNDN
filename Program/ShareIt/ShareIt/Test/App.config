<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="BasicHttpBinding_IAccessRightService" />
                <binding name="BasicHttpBinding_IMediaItemService" />
                <binding name="BasicHttpBinding_IAuthService" />
                <binding name="BasicHttpBinding_ITransferService" messageEncoding="Mtom" />
                <binding name="BasicHttpBinding_IUserService" />
            </basicHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://localhost:1266/AccessRightService.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IAccessRightService"
                contract="ServiceReference1.IAccessRightService" name="BasicHttpBinding_IAccessRightService" />
            <endpoint address="http://localhost:1266/MediaItemService.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IMediaItemService"
                contract="ServiceReference2.IMediaItemService" name="BasicHttpBinding_IMediaItemService" />
            <endpoint address="http://localhost:1266/AuthService.svc" binding="basicHttpBinding"
                bindingConfiguration="BasicHttpBinding_IAuthService" contract="ServiceReference3.IAuthService"
                name="BasicHttpBinding_IAuthService" />
            <endpoint address="http://localhost:1266/TransferService.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_ITransferService"
                contract="ServiceReference4.ITransferService" name="BasicHttpBinding_ITransferService" />
            <endpoint address="http://localhost:1266/UserService.svc" binding="basicHttpBinding"
                bindingConfiguration="BasicHttpBinding_IUserService" contract="ServiceReference5.IUserService"
                name="BasicHttpBinding_IUserService" />
        </client>
    </system.serviceModel>
</configuration>